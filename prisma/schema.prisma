generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user {
  id       Int    @id(map: "PK_cace4a159ff9f2512dd42373760") @default(autoincrement())
  username String @db.VarChar
  email    String @db.VarChar
  password String @db.VarChar
}
